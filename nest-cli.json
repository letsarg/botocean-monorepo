{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/hub/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/hub/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/hub",
  "projects": {
    "hub": {
      "type": "application",
      "root": "apps/hub",
      "entryFile": "main",
      "sourceRoot": "apps/hub/src",
      "compilerOptions": {
        "tsConfigPath": "apps/hub/tsconfig.app.json"
      }
    },
    "axenode": {
      "type": "application",
      "root": "apps/axenode",
      "entryFile": "main",
      "sourceRoot": "apps/axenode/src",
      "compilerOptions": {
        "tsConfigPath": "apps/axenode/tsconfig.app.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "message_bundle": {
      "type": "application",
      "root": "apps/message_bundle",
      "entryFile": "main",
      "sourceRoot": "apps/message_bundle/src",
      "compilerOptions": {
        "tsConfigPath": "apps/message_bundle/tsconfig.app.json"
      }
    }
  }
}