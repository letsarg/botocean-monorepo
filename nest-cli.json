{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/money-printer/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/money-printer/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/money-printer",
  "projects": {
    "money-printer": {
      "type": "application",
      "root": "apps/money-printer",
      "entryFile": "main",
      "sourceRoot": "apps/money-printer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/money-printer/tsconfig.app.json"
      }
    },
    "hub": {
      "type": "application",
      "root": "apps/hub",
      "entryFile": "main",
      "sourceRoot": "apps/hub/src",
      "compilerOptions": {
        "tsConfigPath": "apps/hub/tsconfig.app.json"
      }
    },
    "axenode": {
      "type": "application",
      "root": "apps/axenode",
      "entryFile": "main",
      "sourceRoot": "apps/axenode/src",
      "compilerOptions": {
        "tsConfigPath": "apps/axenode/tsconfig.app.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    }
  }
}